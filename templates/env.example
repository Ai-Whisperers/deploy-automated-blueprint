# ===========================================
# Self-Deploy Configuration
# Copy to .env and fill in values
# ===========================================

# Cloudflare Tunnel
TUNNEL_NAME=my-app
TUNNEL_ID=
DOMAIN=app.example.com

# Service Ports
PORT_FRONTEND=3000
PORT_API=8000
PORT_REDIS=6379

# Database (if applicable)
DATABASE_URL=
REDIS_URL=redis://localhost:6379

# API Keys (add your own)
# OPENAI_API_KEY=
# STRIPE_KEY=

# Environment
APP_ENV=production
NODE_ENV=production
DEBUG=false
LOG_LEVEL=info
